# Environment variables for the backend

# Private key for the Ethereum wallet
PRIVATE_KEY=your_private_key_here

# URL of the Ethereum provider (e.g., Ganache, Infura, Alchemy)
PROVIDER_URL=http://127.0.0.1:8545

# Address of the CarbonCreditToken contract
CCT_ADDRESS=your_cct_contract_address_here

# Address of the ProjectRegistration contract
PR_ADDRESS=your_pr_contract_address_here

# Secret key for JWT authentication
JWT_SECRET=your_jwt_secret_key_here
